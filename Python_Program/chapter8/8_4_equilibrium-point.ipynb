   "source": [
    "import sympy as sym\n",
    "\n",
    "x, y= sym.symbols('x, y')\n",
    "\n",
    "k1 = 0.5\n",
    "k2 = 0.02\n",
    "k3 = 0.004\n",
    "k4 = 0.4\n",
    "\n",
    "eq1 = k1*x-k2*x*y \n",
    "eq2 = k3*x*y-k4*y \n",
    "eqs = sym.solve([eq1,eq2],[x,y])[1]\n",
    "\n",
    "print('x = %d, y = %d ' %(eqs[0], eqs[1]))"
   ]
